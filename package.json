{
  "name": "@chendap/ui",
  "version": "1.0.1",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "es",
    "demo"
  ],
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "homepage": "https://github.com/niaogege/cpp-ui",
  "repository": "https://github.com/niaogege/cpp-ui",
  "bugs": {
    "url": "https://github.com/niaogege/cpp-ui/issues"
  },
  "keywords": [
    "react",
    "component",
    "components",
    "hooks",
    "frontend",
    "ui"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "^16.12.0 || ^17.0.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.0.26",
    "@umijs/fabric": "^2.8.1",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.0.5",
    "classnames": "^2.3.1",
    "dumi": "^2.1.17",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "node-sass": "^7.0.1",
    "prettier": "^2.2.1",
    "sass-loader": "^12.6.0",
    "yorkie": "^2.0.0"
  },
  "license": "ISC"
}
